<div class="wrapper">
  <%= render 'shared/header' %>
    <%= render 'record', pre_enrollment: @pre_enrollment %>
</div>
<%= render 'shared/footer' %>
<% content_for :script do %>
<script>
    mixpanel.track("Acessou pagina editar pré-matrícula", { "semester_pre_enrollment": "<%= @pre_enrollment.semester %>" });
  
    $("#goToTop").click(function() {
          $('html,body').animate({
              scrollTop: $("#header").offset().top
          }, 500);
    });
</script>
<% end %>